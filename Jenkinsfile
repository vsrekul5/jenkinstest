pipeline {
    agent any

    stages {
        
        stage('Testing') {
            steps {
                echo Testing : The Artifact will be tested'
            }
        }
        stage('build') {
            steps {
                echo 'build:The Artifact will be tested'
            }
        }
       stage('Deploy') {
            steps {
                echo 'Deploy: The software will now be deployed!'
            }
        }
    }  
}
